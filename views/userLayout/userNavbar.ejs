<header class="header header-6">
    <div class="header-top">
        <div class="container">
            
            <div class="header-right">
                      
                    </div><!-- End .header-menu -->
                </div><!-- End .header-dropdown -->
            </div><!-- End .header-right -->
        </div>
    </div>
    <div class="header-middle">
        <div class="container">
            <div class="header-left">
                <div class="header-search header-search-extended header-search-visible d-none d-lg-block">
                    <a href="#" class="search-toggle" role="button"><i class="icon-search"></i></a>
                    <form action="#" method="get">
                        <div class="header-search-wrapper search-wrapper-wide">
                            <label for="q" class="sr-only">Search</label>
                            <button class="btn btn-primary" type="submit"><i class="icon-search"></i></button>
                            <input type="search" class="form-control" name="q" id="q" placeholder="Search product ..." required>
                        </div><!-- End .header-search-wrapper -->
                    </form>
                </div><!-- End .header-search -->
            </div>
            <div class="header-center">
                <a href="/" class="logo">
                    <img src="/userAssets/images/munna agro.png" alt="DRESS LOGO" width="82" height="20">
                </a>
            </div><!-- End .header-left -->

            <div class="header-right">
                <div class="dropdown cart-dropdown">

                    <% if (session==null) { %>

                        <a href="/login" class="dropdown-logos" role="button">
                            <i class="icon-user"></i>
                           
                        </a>
                        <% } else { %>
                        <a href="#" class="dropdown-logos" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"
                            data-display="static">
                            <i class="icon-user"></i>
                            <strong><%= user.name %></strong>
                        </a>
                    
                        <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
                            <li><a class="dropdown-item" href="/profile">Profile</a></li>
                            <li><a class="dropdown-item" href="/logout">Logout</a></li>
                        </ul>
                        <% } %>
                    


        </div>


                  <!-- WISHLIST BUTTON -->
                <a href="/wishlist" class="wishlist-link">
                    <i class="icon-heart-o"></i>
                    <span class="wishlist-txt">My Wishlist</span>
                </a>


                <!-- CART BUTTON -->
                <div class="dropdown cart-dropdown">
                    <a href="/cart" class="dropdown-toggle" >
                        <i class="icon-shopping-cart"></i>
                        <span class="cart-txt"></span>
                    </a>

                   
                    </div><!-- End .dropdown-menu -->
                </div><!-- End .cart-dropdown -->
            </div>
        </div><!-- End .container -->
    </div><!-- End .header-middle -->

    <div class="header-bottom sticky-header">
        <div class="container">
            <div class="header-left">
                <nav class="main-nav">
                    <ul class="menu sf-arrows">
                        <li class="megamenu-container active">
                            <a href="/" class="sf-with-ul">Home</a>

                            
                        </li>
                        <li>
                            <a href="/allProducts" class="sf-with-ul">Shop</a>

                            
                        </li>
                    
                               
                         
                      
                    </ul><!-- End .menu -->
                </nav><!-- End .main-nav -->

                <button class="mobile-menu-toggler">
                    <span class="sr-only">Toggle mobile menu</span>
                    <i class="icon-bars"></i>
                </button>
            </div><!-- End .header-left -->

           
        </div><!-- End .container -->
    </div><!-- End .header-bottom -->
</header><!-- End .header -->